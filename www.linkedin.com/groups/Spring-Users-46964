 // global webtrack object for timing information
 var WEBTRACK_GLOBAL = {
 softTimings: {},
 hardTimings: {},
 seedTime: null,
 now: function() { return (new Date()).getTime(); },
 lapse: function() { return this.now() - this.seedTime; },
 get: function(name) { return (this.hardTimings.hasOwnProperty(name)) ? this.hardTimings[name] : this.softTimings[name]; },
 getSoftTimings: function() { return this.softTimings; },
 set: function(name, val) { this.hardTimings[name] = (val*-1 > 0) ? null : val*1; },
 record: function(name) { this.softTimings[name] = this.now(); },
 clean: function() {} // FIXME
 };
 WEBTRACK_GLOBAL.seedTime = WEBTRACK_GLOBAL.now();
 </script>
