				function changeCSSClasses(activeList,inactiveList,activeStyle,inactiveStyle){
					var theActiveList = activeList.split(',');
					var theInactiveList = inactiveList.split(',');
					if(!activeStyle){var activeStyle='active'}
					if(!inactiveStyle){var inactiveStyle='inactive'}
					
					for(var i = 0; i < theActiveList.length; i++){
						$(theActiveList[i]).attr('class',activeStyle);
					}
					for(var i = 0; i < theInactiveList.length; i++){
						$(theInactiveList[i]).attr('class',inactiveStyle);
					}
				}
				
				function showHideWidgetDivs(showList,hideList){
					var theShowList = showList.split(',');
					var theHideList = hideList.split(',');
					
					for(var i = 0; i < theShowList.length; i++){
						$(theShowList[i]).show();
					}
					for(var i = 0; i < theHideList.length; i++){
						$(theHideList[i]).hide();
					}
				}
			
			</script>
