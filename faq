<div class='content_inner'>
<h1>Frequently Asked Questions</h1>
<script>
  //<![CDATA[
    $(document).ready(function(){
      $("a#toggle").click(function(){
       $(this).hide(); 
       $(this).next().show();
      });
      $("a#toggle2").click(function(){
       $(this).parent().hide();
       $(this).parent().prev().show(); 
      });
